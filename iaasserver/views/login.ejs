<% include ./layouts/head %>
<div class="login-form">
     <img src="img/Logo_bubook.png" id="logo_bubook"></img>
     <div class="form-group ">
       <input type="text" class="form-control" placeholder="Nombre de usuario " id="UserName">
       <i class="fa fa-user"></i>
     </div>
     <div class="form-group log-status">
       <input type="password" class="form-control" placeholder="Contraseña " id="Password">
       <i class="fa fa-lock"></i>
     </div>
      <span class="alert">Datos erróneos</span>
      <a class="link" href="#">¿Has olvidado la contraseña?</a>
     <button type="button" class="log-btn" id="boton_local">Iniciar sesión</button>
     <button type="button" class="log-btn" id="boton_github">Iniciar con Github &nbsp;<i class="fa fa-github fa-lg" aria-hidden="true"></i></button>


   </div>
<% include ./layouts/footer %>